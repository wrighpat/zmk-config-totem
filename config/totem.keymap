#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/outputs.h>

// Home row mods macro
#define HRML(k1,k2,k3,k4) &ht LGUI k1  &ht LALT k2  &ht LCTRL k3 &ht LSHIFT k4
#define HRMR(k1,k2,k3,k4) &ht LSHFT k1  &ht RCTL k2  &ht RALT k3 &ht RGUI k4
#define MHRML(k1,k2,k3,k4) &ht LCTRL k1  &ht LALT k2  &ht LGUI k3 &ht LSHIFT k4
#define MHRMR(k1,k2,k3,k4) &ht LSHFT k1  &ht RGUI k2  &ht RALT k3 &ht RCTL k4

#define BASE 0
#define MAC 1
#define NUM  2
#define SYM  3
#define ADJ  4
#define SHT 5


&mt {
 
  flavor = "tap-preferred";
  tapping-term-ms = <200>;
quick-tap-ms = <125>;
};

/ {
		

	    ht: hold-tap {
            compatible = "zmk,behavior-hold-tap";
            label = "hold_tap";
            #binding-cells = <2>;
            flavor = "tap-preferred";
            tapping-term-ms = <200>;
            quick-tap-ms = <125>;
            bindings = <&kp>, <&kp>;
        };
	
		
		ltq: layer_tap_quick {
            compatible = "zmk,behavior-hold-tap";
            label = "LAYER_TAP_QUICK";
            #binding-cells = <2>;
            flavor = "balanced";
            tapping-term-ms = <200>;                        
            quick-tap-ms = <175>;
            bindings = <&mo>, <&kp>;
        };
		
		meh: meh_kp {
            compatible = "zmk,behavior-hold-tap";
            label = "MEH_KP";
            #binding-cells = <2>;
            flavor = "tap-preferred";
            tapping-term-ms = <200>;                        
            quick-tap-ms = <175>;
            bindings = <&sk>, <&kp>;
        };


 	macros {

        };

        
        //tap dance
		gs: tap_dance_0 {
			compatible = "zmk,behavior-tap-dance";
			label = "GREENSHOT";
			#binding-cells = <0>;
			tapping-term-ms = <200>;
			bindings = <&kp PSCRN>, <&kp LA(PSCRN)>;
        };
		
		fr: tap_dance_1 {
			compatible = "zmk,behavior-tap-dance";
			label = "FIND_REPLACE";
			#binding-cells = <0>;
			tapping-term-ms = <200>;
			bindings = <&kp LC(F)>, <&kp LC(H)>;
        };
		


    combos {
        compatible = "zmk,combos";
       
       combo_esc {
            timeout-ms = <50>;
            key-positions = <0 1>;
            bindings = <&kp ESC>;
        };

		
		combo_space {
            timeout-ms = <50>;
            key-positions = <23 24>;
            bindings = <&kp SPACE>;
        };

		combo_capsWord {
            timeout-ms = <50>;
            key-positions = <10 11>;
            bindings = <&caps_word>;
        };

		
    };



        keymap {
                compatible = "zmk,keymap";
// ▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄

